<document>
    <testcase>
        <about>Test conjuncts.</about>
        <input>Gretchen and her daughter bought motor oil, spark plugs, and dynamite.</input>
        <output>
            <set>
                <relation type="bought" tail="Gretchen" head="motor oil"/>
                <relation type="bought" tail="Gretchen" head="spark plugs"/>
                <relation type="bought" tail="Gretchen" head="dynamite"/>
                <relation type="bought" tail="her daughter" head="motor oil"/>
                <relation type="bought" tail="her daughter" head="spark plugs"/>
                <relation type="bought" tail="her daughter" head="dynamite"/>
            </set>
        </output>
    </testcase>

    <testcase>
        <about>Test appositional modifiers</about>
        <input>Wheat is used for making wheat flour, a typical ingredient of bread dough.</input>
        <output>
            <set>
                <relation type="makes" tail="Wheat" head="wheat flour"/>
                <relation type="is a" tail="wheat flour" head="typical ingredient of bread dough"/>
            </set>
        </output>
    </testcase>
</document>